// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Imoveis {

    id Int @id @default(autoincrement())
    valor Int
    titulo String
    endereco String
    especif String
    tipo String
    informacoes String

}